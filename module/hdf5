wrapper_prefix=../
exec_prefix=${wrapper_prefix}
wrapper_libdir=../
Name:wrapper-config
Description: the static wrapper
Version: 1

Libs:-Wl,--wrap=H5Fcreate -Wl,--wrap=H5Fopen -Wl,--wrap=H5Fclose  -L${../mywrapper} -l{hdf5} -l{wrap../mywrapper/wrapper-config.pc} 

Libs.private:-Wl,--wrap=H5Fcreate -Wl,--wrap=H5Fopen -Wl,--wrap=H5Fclose 
